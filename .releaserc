{
  "branches": ["main", "release/**"],
  "tagFormat": ["release/${version}"],
  "plugins": [
        "@semantic-release/commit-analyzer",
        "@semantic-release/github",
        [
        "@semantic-release/exec",
        {
            "verifyReleaseCmd": "echo \"NEXT_RELEASE_VERSION=${nextRelease.version}\" >> $GITHUB_ENV"
        }
        ]
    ]
}